/* pages/books_basket/books_basket.wxss */
.line{
  width: 100%;
  height: 10rpx;
  background-color: rgb(209, 209 ,209);
  margin-bottom: 5rpx;
}
.product_list{
  display: flex;
  flex-direction: row;
  width: 96%;
  height: 150rpx;
  align-self: center;
  align-items: center;
  justify-content: space-between;
}
.product_list_1{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-left: 30rpx;
}
.product_list_2{
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
  justify-content: space-between;
}
.product_list_3{
  display: flex;
  flex-direction: row;
  margin-left: 20rpx;
  justify-content: space-between;
}
.anniu{
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  justify-content: space-between;
  width: 100%;
  background-color: white;
  align-items: center;
}
.pay{
  display: flex;
  flex-direction: row;
  align-items: center;
  align-self: center;
  justify-content: center;
  width: 100%;
  height: 100%;
}